<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - web_test_lms</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@24,400,0,0" />
    <link rel="stylesheet" href="~/css/layout.css" asp-append-version="true" />
    <script src="~/js/layout.js" defer asp-append-version="true"></script>
</head>
<body>
    <aside class="sidebar">
		<header class="sidebar-header">
			<a href="#" class="header-logo">
                <img src="~/images/logo.jpg" alt="PND" />
            </a>
            <button class="sidebar-toggler">
                <span class="material-symbols-rounded">
                chevron_left
                </span>
            </button>
		</header>

        <nav class="sidebar-nav">
			<ul class="nav-list primary-nav">
				<li class="nav-item1">
					<a asp-controller="Home" asp-action="Index" class="nav-link1">
						<span class="material-symbols-rounded">
                            home
						</span>
						<span class="nav-label">HOME</span>
					</a>
                    <ul class="dropdown-menu1">
                        <li class="nav-item1">
                            <a class="nav-link1 dropdown-title">
                                HOME
                            </a>
                        </li>
                    </ul>
				</li>

                <li class="nav-item1">
                    <a asp-controller="Home" asp-action="Index" class="nav-link1">
                        <span class="material-symbols-rounded">
                            rocket
                        </span>
                        <span class="nav-label">JOB</span>
                    </a>
                    <ul class="dropdown-menu1">
                        <li class="nav-item1">
                            <a class="nav-link1 dropdown-title">
                                JOB
                            </a>
                        </li>
                    </ul>
                </li>

                <li class="nav-item1">
                    <a asp-controller="Home" asp-action="Index" class="nav-link1">
                        <span class="material-symbols-rounded">
                            contract_edit
                        </span>
                        <span class="nav-label">QUOTATION</span>
                    </a>
                    <ul class="dropdown-menu1">
                        <li class="nav-item1">
                            <a class="nav-link1 dropdown-title">
                                QUOTATION
                            </a>
                        </li>
                    </ul>
                </li>

                <li class="nav-item1">
                    <a asp-controller="Home" asp-action="Index" class="nav-link1">
                        <span class="material-symbols-rounded">
                            payments
                        </span>
                        <span class="nav-label">TẠM ỨNG THANH TOÁN</span>
                    </a>
                    <ul class="dropdown-menu1">
                        <li class="nav-item1">
                            <a class="nav-link1 dropdown-title">
                                TẠM ỨNG THANH TOÁN
                            </a>
                        </li>
                    </ul>
                </li>

                <li class="nav-item1">
                    <a asp-controller="Home" asp-action="Index" class="nav-link1">
                        <span class="material-symbols-rounded">
                            home
                        </span>
                        <span class="nav-label">HS CODE</span>
                    </a>
                    <ul class="dropdown-menu1">
                        <li class="nav-item1">
                            <a class="nav-link1 dropdown-title">
                                HS CODE
                            </a>
                        </li>
                    </ul>
                </li>
				<!--dropdown--> 
                <li class="nav-item1 dropdown-container">
                    <a href="#" class="nav-link1 dropdown-toggle1">
                        <span class="material-symbols-rounded">
                            Menu
                        </span>
                        <span class="nav-label">MENU</span>
						<span class="dropdown-icon material-symbols-rounded">
							keyboard_arrow_down
						</span>
                    </a>
					<ul class="dropdown-menu1">
                        <li class="nav-item1">
                            <a class="nav-link1 dropdown-title">
                                MENU
                            </a>
                        </li>
                        <li class="nav-item1">
                            <a asp-controller="Agents" asp-action="Index" class="nav-link1 dropdown-link">
                                <span class="material-symbols-rounded">
                                    support_agent
                                </span>
                                Agent
                            </a>
                        </li>
						<li class="nav-item1">
							<a asp-controller="Home" asp-action="Index" class="nav-link1 dropdown-link">
                                CNEE
							</a>
						</li>
						<li class="nav-item1">
                            <a asp-controller="Home" asp-action="Index" class="nav-link1 dropdown-link">
                                <span class="material-symbols-rounded">
                                    local_shipping
                                </span>
                                SHIPPER
							</a>
						</li>
                        <li class="nav-item1">
                            <a asp-controller="Home" asp-action="Index" class="nav-link1 dropdown-link">
                                <span class="material-symbols-rounded">
                                    groups
                                </span>
                                CUSTOMER
                            </a>
                        </li>
                        <li class="nav-item1">
                            <a asp-controller="Home" asp-action="Index" class="nav-link1 dropdown-link">
                                <span class="material-symbols-rounded">
                                    directions_boat
                                </span>
                                CARRIER
                            </a>
                        </li>
                        <li class="nav-item1">
                            <a asp-controller="Home" asp-action="Index" class="nav-link1 dropdown-link">
                                <span class="material-symbols-rounded">
                                    handshake
                                </span>
                                SUPPLIER
                            </a>
                        </li>
					</ul>
                </li>

                <li class="nav-item1 dropdown-container">
                    <a href="#" class="nav-link1 dropdown-toggle1">
                        <span class="material-symbols-rounded">
                            Menu
                        </span>
                        <span class="nav-label">DANH MỤC</span>
                        <span class="dropdown-icon material-symbols-rounded">
                            keyboard_arrow_down
                        </span>
                    </a>
                    <ul class="dropdown-menu1">
                        <li class="nav-item1">
                            <a class="nav-link1 dropdown-title">
                                DANH MỤC
                            </a>
                        </li>
                        <li class="nav-item1">
                            <a asp-controller="Cports" asp-action="Index" class="nav-link1 dropdown-link">
                                <span class="material-symbols-rounded">
                                    anchor
                                </span>
                                PORT
                            </a>
                        </li>
                        <li class="nav-item1">
                            <a asp-controller="Currencies" asp-action="Index" class="nav-link1 dropdown-link">
                                <span class="material-symbols-rounded">
                                    anchor
                                </span>
                                CURRENCY
                            </a>
                        </li>
                        <li class="nav-item1">
                            <a asp-controller="Cports" asp-action="Index" class="nav-link1 dropdown-link">
                                <span class="material-symbols-rounded">
                                    anchor
                                </span>
                                UNIT
                            </a>
                        </li>
                    </ul>
                </li>
            </ul>


			<ul class="nav-list secondary-nav">
				<li class="nav-item1">
                    <a asp-controller="Home" asp-action="Index" class="nav-link1">
						<span class="material-symbols-rounded">
                            help
						</span>
						<span class="nav-label">Support</span>
					</a>

                    <ul class="dropdown-menu1">
                        <li class="nav-item1">
                            <a class="nav-link1 dropdown-title">
                                Support
                            </a>
                        </li>
                    </ul>
				</li>
                <li class="nav-item1">
                    <a asp-controller="Home" asp-action="Index" class="nav-link1">
                        <span class="material-symbols-outlined">
                            account_circle
                        </span>
                        <span class="nav-label">ACCOUNT</span>
                    </a>
                    <ul class="dropdown-menu1">
                        <li class="nav-item1">
                            <a class="nav-link1 dropdown-title">
                                ACCOUNT
                            </a>
                        </li>
                    </ul>
                </li>
                <li class="nav-item1">
                    <a asp-controller="Home" asp-action="Index" class="nav-link1">
                        <span class="material-symbols-rounded">
                            logout
                        </span>
                        <span class="nav-label">LOGOUT</span>
                    </a>
                    <ul class="dropdown-menu1">
                        <li class="nav-item1">
                            <a class="nav-link1 dropdown-title">
                                LOGOUT
                            </a>
                        </li>
                    </ul>
                </li>
            </ul>
        </nav>
    </aside>


    <main class="main">
        <div class="">
            <h1>MORE THAN PND</h1>
        </div>

        <div class="container-main">
            @RenderBody()
        </div>
    </main>

    @RenderSection("Scripts", required: false)
</body>
</html>
