                <img src="~/images/logo.jpg" alt="PND" />
            </a>
            <button class="sidebar-toggler">
                <span class="material-symbols-rounded">
                chevron_left
                </span>
            </button>
		</header>

        <nav class="sidebar-nav">
			<ul class="nav-list primary-nav">
				<li class="nav-item">
					<a asp-controller="Home" asp-action="Index" class="nav-link">
						<span class="material-symbols-rounded">
                            home
						</span>
						<span class="nav-label">Home</span>
					</a>
                    <ul class="dropdown-menu">
                        <li class="nav-item">
                            <a class="nav-link dropdown-title">
                                Home
                            </a>
                        </li>
                    </ul>
				</li>

                <li class="nav-item">
                    <a asp-controller="Home" asp-action="Index" class="nav-link">
                        <span class="material-symbols-rounded">
                            home
                        </span>
                        <span class="nav-label">JOB</span>
                    </a>
                    <ul class="dropdown-menu">
                        <li class="nav-item">
                            <a class="nav-link dropdown-title">
                                JOB
                            </a>
                        </li>
                    </ul>
                </li>

                <li class="nav-item">
                    <a asp-controller="Home" asp-action="Index" class="nav-link">
                        <span class="material-symbols-rounded">
                            home
                        </span>
                        <span class="nav-label">QUOTATION</span>
                    </a>
                    <ul class="dropdown-menu">
                        <li class="nav-item">
                            <a class="nav-link dropdown-title">
                                QUOTATION
                            </a>
                        </li>
                    </ul>
                </li>

                <li class="nav-item">
                    <a asp-controller="Home" asp-action="Index" class="nav-link">
                        <span class="material-symbols-rounded">
                            home
                        </span>
                        <span class="nav-label">HS CODE</span>
                    </a>
                    <ul class="dropdown-menu">
                        <li class="nav-item">
                            <a class="nav-link dropdown-title">
                                HS CODE
                            </a>
                        </li>
                    </ul>
                </li>
				<!--dropdown--> 
                <li class="nav-item dropdown-container">
                    <a href="#" class="nav-link dropdown-toggle">
                        <span class="material-symbols-rounded">
                            Menu
                        </span>
                        <span class="nav-label">Menu</span>
						<span class="dropdown-icon material-symbols-rounded">
							keyboard_arrow_down
						</span>
                    </a>
					<ul class="dropdown-menu">
                        <li class="nav-item">
                            <a class="nav-link dropdown-title">
                                MENU
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-controller="Agents" asp-action="Index" class="nav-link dropdown-link">
